        <Item Value="street"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="newContactIterator" id="city">
      <AttrNames>
        <Item Value="city"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="newContactIterator" id="zip">
      <AttrNames>
        <Item Value="zip"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="newContactIterator" id="country">
      <AttrNames>
        <Item Value="country"/>
      </AttrNames>
    </attributeValues>
