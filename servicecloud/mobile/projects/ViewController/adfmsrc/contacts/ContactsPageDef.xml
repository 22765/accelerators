<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.23.11" id="ContactsPageDef"
                Package="Contacts">
  <parameters/>
  <executables>
    <invokeAction id="initReport" Binds="initContactReport"/>
    <invokeAction id="initFilters" Binds="initSearchCriteria"/>
    <variableIterator id="variables"/>
    <iterator Binds="root" RangeSize="15" DataControl="ContactReportController" id="ReportControllerIterator"
              CacheResults="false" Refresh="always"/>
    <accessorIterator MasterBinding="ReportControllerIterator" Binds="report" RangeSize="15"
                      DataControl="ContactReportController" BeanClass="report.ReportItem" id="reportIterator"
                      CacheResults="false" Refresh="always"/>
    <iterator Binds="root" RangeSize="25" DataControl="ContactSearchFiltersController"
              id="ContactSearchFiltersControllerIterator"/>
    <accessorIterator MasterBinding="ContactSearchFiltersControllerIterator" Binds="filters" RangeSize="25"
                      DataControl="ContactSearchFiltersController" BeanClass="contacts.ContactSearchFilters"
                      id="filtersIterator"/>
  </executables>
  <bindings>
    <methodAction id="gotoSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <methodAction id="gotoFeature" RequiresUpdateModel="true" Action="invokeMethod" MethodName="gotoFeature"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider">
      <NamedData NDName="featureId" NDValue="Springboard" NDType="java.lang.String"/>
    </methodAction>
    <tree IterBinding="reportIterator" id="report">
      <nodeDefinition DefName="report.ReportItem" Name="report0">
        <AttrNames>
          <Item Value="attr1"/>
          <Item Value="attr2"/>
          <Item Value="attr3"/>
          <Item Value="attr4"/>
          <Item Value="id"/>
          <Item Value="attr5"/>
          <Item Value="attr6"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction id="initContactReport" RequiresUpdateModel="true" Action="invokeMethod" MethodName="initReport"
                  IsViewObjectMethod="false" DataControl="ContactReportController"
                  InstanceName="data.ContactReportController.dataProvider">
      <NamedData NDName="reportName" NDValue="AcceleratorContactList" NDType="java.lang.String"/>
      <NamedData NDName="dataControl" NDValue="ContactReportController" NDType="java.lang.String"/>
    </methodAction>
    <methodAction id="toggleSpringboard" RequiresUpdateModel="true" Action="invokeMethod" MethodName="toggleSpringboard"
                  IsViewObjectMethod="false" DataControl="ApplicationFeatures"
                  InstanceName="data.ApplicationFeatures.dataProvider"/>
    <attributeValues IterBinding="filtersIterator" id="orgName">
      <AttrNames>
        <Item Value="orgName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="filtersIterator" id="email">
      <AttrNames>
        <Item Value="email"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="filtersIterator" id="phone">
      <AttrNames>
        <Item Value="phone"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="filtersIterator" id="firstName">
      <AttrNames>
        <Item Value="firstName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="filtersIterator" id="lastName">
      <AttrNames>
        <Item Value="lastName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="filtersIterator" id="assetSerialNumber">
      <AttrNames>
        <Item Value="assetSerialNumber"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="LaunchScanner" RequiresUpdateModel="true" Action="invokeMethod" MethodName="LaunchScanner"
                  IsViewObjectMethod="false" DataControl="BarcodeScannerBean"
                  InstanceName="data.BarcodeScannerBean.dataProvider"
                  ReturnName="data.BarcodeScannerBean.methodResults.LaunchScanner_BarcodeScannerBean_dataProvider_LaunchScanner_result"/>
    <methodAction id="doReset" RequiresUpdateModel="true" Action="invokeMethod" MethodName="doReset"
                  IsViewObjectMethod="false" DataControl="ContactReportController"
                  InstanceName="data.ContactReportController.dataProvider">
      <NamedData NDName="filters" NDValue="#{bindings.ContactSearchFiltersControllerIterator.currentRow.dataProvider}"
                 NDType="java.lang.Object"/>
    </methodAction>
    <methodAction id="doSearch" RequiresUpdateModel="true" Action="invokeMethod" MethodName="doSearch"
                  IsViewObjectMethod="false" DataControl="ContactReportController"
                  InstanceName="data.ContactReportController.dataProvider">
      <NamedData NDName="filters" NDValue="#{bindings.ContactSearchFiltersControllerIterator.currentRow.dataProvider}"
                 NDType="java.lang.Object"/>
    </methodAction>
    <methodAction id="doCancel" RequiresUpdateModel="true" Action="invokeMethod" MethodName="doCancel"
                  IsViewObjectMethod="false" DataControl="ContactReportController"
                  InstanceName="data.ContactReportController.dataProvider">
      <NamedData NDName="filters" NDValue="#{bindings.ContactSearchFiltersControllerIterator.currentRow.dataProvider}"
                 NDType="java.lang.Object"/>
    </methodAction>
    <methodAction id="initSearchCriteria" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="initSearchCriteria" IsViewObjectMethod="false" DataControl="ContactReportController"
                  InstanceName="data.ContactReportController.dataProvider">
      <NamedData NDName="filters" NDValue="#{bindings.ContactSearchFiltersControllerIterator.currentRow.dataProvider}"
                 NDType="java.lang.Object"/>
    </methodAction>
  </bindings>
</pageDefinition>
